<plugin name="hatching01">
	
	<vbox>
	<preview/>
	<notebook>
		<tab label="Params">
			<vbox>
			<frame label="Propability">
				<vbox>
					<param name="proba" type="double" default="0.05" label="Propability" min="0.0" max="1.0" step="0.0001"/>
				</vbox>
			</frame>
			<frame label="Length">
				<vbox>
					<param name="minlen" type="double" default="80.0" label="Min. Length" min="0.01" max="5000.0"/>
					<param name="maxlen" type="double" default="100.0" label="Max. Length" min="0.01" max="5000.0"/>
				</vbox>
			</frame>
			<frame label="Alpha">
				<vbox>
					<param name="minalpha" type="double" default="0.7" label="Min. Alpha" min="0.01" max="1.0" step="0.001"/>
					<param name="maxalpha" type="double" default="0.9" label="Max. Alpha" min="0.01" max="1.0" step="0.001"/>
				</vbox>
			</frame>
			<frame label="Line Width">
				<vbox>
					<param name="minlinewidth" type="double" default="0.1" label="Min. Line Width" min="0.001" max="100.0" step="0.001"/>
					<param name="maxlinewidth" type="double" default="1.0" label="Max. Line Width" min="0.001" max="100.0" step="0.001"/>
				</vbox>
			</frame>	
			</vbox>
		</tab>
	</notebook>
	</vbox>
</plugin>
