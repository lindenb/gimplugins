<plugin name="hatching03">
	
	<vbox>
	<preview/>
	<notebook>
		<tab label="Params">
			<vbox>
			<frame label="Propability">
				<vbox>
					<param name="iterations" type="int" default="500" label="Iterations" min="1" max="10000" step="1"/>
				</vbox>
				<vbox>
					<param name="proba" type="double" default="0.05" label="Propability" min="0.0" max="1.0" step="0.0001"/>
				</vbox>
			</frame>
			
			<minmax label="Distance between strokes" type="double" name="diststroke" value="1" min="0" max="50"/>
			<minmax label="Gray" type="double" name="gray" value="1.0" min="0.0" max="1.0"/>
			<minmax label="Stroke Size" type="double" name="strokesize" value="1.0" min="20.0" max="25.0"/>			
			<minmax label="Stroke Width" type="double" name="strokewidth" value="1.0" min="0.5" max="5.0"/>			
			<minmax label="Distance between circles" type="double" name="distcircle" value="0.1" min="0" max="50"/>
			
			<frame id='alpha'/>
			<frame id='line-width'/>
			</vbox>
		</tab>
		<tab label="Circle">
			<vbox>
				<param label="Center X (%)" type="double" name="centerx" default="0.5" min="0.0" max="1.0"/>
				<param label="Center Y (%)" type="double" name="centery" default="0.5" min="0.0" max="1.0"/>
			</vbox>
		</tab>
	</notebook>
	</vbox>
</plugin>
